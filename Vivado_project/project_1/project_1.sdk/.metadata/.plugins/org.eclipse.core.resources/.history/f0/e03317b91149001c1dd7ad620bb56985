#include "xparameters.h"
#include "NeuralNetwork.hpp"

int8_t bias_[] = {
	#include "bias.dat"
};

int8_t weights_[] = {
	#include "weights.dat"
};

uint8_t getChoiceFromUser()
{
	xil_printf("Enter choice: \r\n");
	/* Read an input value from the console. */
	uint8_t value = inbyte();
	xil_printf("\r\n\r\n");

	return value-48;
}

int main()
{
	int input[80] = { -11, 10, 20, -15, 4, 40, 9, 1, -12, -13, 10, -18, -3, 11, 10, 4, 2, 5, 3, -3, 7, -9, 3, 9, -1, 8, -12, 1, 10, -6, 2, -3, -5, 3, 1, -2, 2, -1, 3, 1, -4, 0, -1, 3, -3, 2, -5, -2, 5, 5, -3, -2, 1, 2, -3, -4, -5, -5, -1, 0, 3, 3, 0, 2, 4, 4, 5, -1, 3, -1, 2, 1, 2, -3, 1, 0, -1, -2, 1, 0};
	int input1[80] = {21, 45, -6, 15, 15, 19, -18, 7, -2, 19, -2, 11, 11, -5, -16, 9, -9, -18, 5, 15, -3, -4, -2, 11, -6, 6, -9, 7, 9, -1, 1, 3, 2, -1, -1, -10, 0, -2, -2, -2, 1, -1, 1, -6, -3, 5, -5, 2, -1, 0, -1, -8, -3, 0, -1, 3, 0, -4, 1, -1, 0, -4, -5, -2, 2, 1, 0, -2, -2, 3, -2, 1, -1, -1, -4, -1, 2, -4, -1, 0};

	NeuralNetwork nn = NeuralNetwork();

	nn.setBias((char*)bias_);
	nn.setWeights((char*)weights_);

	nn.runNN((char*) input);

	xil_printf("Done\r\n");
	xil_printf("Done\r\n");
	xil_printf("Done\r\n");


	while(1){
		uint8_t choice = getChoiceFromUser();

	}
	xil_printf("Done\r\n");
